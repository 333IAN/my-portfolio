{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS"
    },
    "deploy": {
        "startCommand": "python manage.py migrate && gunicorn portcontact.wsgi:application --bind 0.0.0.0:$PORT --timeout 120 --workers 1",
        "restartPolicyType": "ON_FAILURE"
    }
}